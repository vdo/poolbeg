      "title": "Total RPS",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "gridPos": { "h": 5, "w": 3, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum(rate(poolbeg_requests_total{chain=~\"$chain\"}[$__rate_interval])) + sum(rate(poolbeg_cache_hits_total{chain=~\"$chain\"}[$__rate_interval]))",
          "legendFormat": "RPS",
          "refId": "A"
